<h1>Update your Article</h1>

<%= form_for @article do |a|%>
<p>
<strong><%= a.label :Article_Name%></strong>
<%= a.text_field :name%>
</p>
<p>
<strong><%= a.label :Description%></strong>
<%= a.text_area :description%>
</p>

<p>
<%= a.submit "Update Article" %>
</p> 
<%end%>
<%= link_to "Back" , :back%>